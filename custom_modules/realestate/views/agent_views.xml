<odoo>
    <!-- Agent Form View -->
    <record id="view_real_estate_agent_form" model="ir.ui.view">
        <field name="name">real.estate.agent.form</field>
        <field name="model">real.estate.agent</field>
        <field name="arch" type="xml">
            <form string="Agent">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="email"/>
                    </group>
                    <notebook>
                        <page string="Properties">
                            <field name="properties"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
